<%= form_tag login_path, method: :post do %>
  <%= label_tag "Email" %><br>
  <%= email_field_tag :email, autocomplete="", required: true %><br><br>

  <%= label_tag "Password" %><br>
  <%= password_field_tag :password, autocomplete="", required: true %><br><br>

  <%= submit_tag "Log In" %><br>
<% end %>

<p><%= flash[:message] %><p>
