<%= form_tag login_path, method: :post do %>
  <%= label_tag "Email" %><br>
  <%= email_field_tag :email, '', required: true, autocomplete: "disabled" %><br><br>

  <%= label_tag "Password" %><br>
  <%= password_field_tag :password '', required: true, autocomplete: "disabled" %><br><br>

  <%= submit_tag "Log In" %><br><br>
<% end %>

<p><%= flash[:message] %><p>
