<%= form_for(@user) do |f| %>
  <%= f.label "Full Name" %>
  <%= f.text_field :name %>

  <%= f.label "Email Address" %>
  <%= f.email_field :email %>

  <%= f.label "Password"%>
  <%= f.password_field :password %>

  <%= f.label "Password Confirmation"%>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Sign Up" %>
<% end %>
